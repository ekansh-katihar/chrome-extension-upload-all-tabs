{
  "name": "Upload Tabs to S3 ",
  "description": "Ever need to save all the tabs for your later use because you web subscription is ending. This will read all the tabs and upload it to S3.",
  "version": "1.0.0",
  "manifest_version": 3,
  "commands": {
    "upload": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "Command+Shift+2"
      },
      "description": "Upload Tabs to S3"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting","tabs"],
  "host_permissions": [ "\u003Call_urls>" ],
  "action": {
    "default_popup": "popup.html"
  }
}
